<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!-- Inherit Form View to Modify it -->
        <record id="inherit_attendance_tree_view_attendance_tree" model="ir.ui.view">
            <field name="name">inherit_attendance_tree_view_attendance_tree</field>
            <field name="model">hr.attendance</field>
            <field name="inherit_id" ref="hr_attendance.view_attendance_tree"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='check_out']" position="after">
                    <field name="break_time" class="text-center"/>
                    <field name="net_worked_hours" widget="float_time"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>